{
  "name": "forest-monitor",
  "version": "0.0.1",
  "description": "a focus monitor",
  "author": "Sad Yashica",
  "scripts": {
    "all-tags": "npm run build-ts && node dist/tools/all-tags.js",
    "all-habits": "npm run build-ts &&  node dist/tools/all-habits.js",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/app.js",
    "watch-node": "nodemon dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/app.js"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/colors": "^1.1.3",
    "@types/cors": "^2.8.4",
    "@types/dateformat": "^1.0.1",
    "@types/ejs": "^2.6.2",
    "@types/express": "^4.11.1",
    "@types/lodash": "^4.14.121",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.35",
    "@types/nconf": "0.0.37",
    "@types/node": "^7.0.50",
    "@types/request-promise": "^4.1.41",
    "cors": "^2.8.5",
    "install": "^0.10.4",
    "moment": "^2.24.0",
    "nodemon": "^1.13.0",
    "npm": "^5.6.0",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "dateformat": "^3.0.3",
    "ejs": "^2.6.1",
    "express": "^4.16.2",
    "firebase": "^5.8.3",
    "lodash": "^4.17.11",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  }
}
